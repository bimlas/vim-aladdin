Execute (Create object):
  let obj = aladdin#sources#PROTOTYPE#define()

Execute (Enabled for every filetype by default):
  let obj.whitelist = []
  let obj.blacklist = []
  AssertEqual obj._EnabledForFiletype('text'), 1
  AssertEqual obj._EnabledForFiletype('asciidoc'), 1

Execute (Whitelist: enabled for certain filetypes):
  let obj.whitelist = ['text']
  let obj.blacklist = []
  AssertEqual obj._EnabledForFiletype('text'), 1
  AssertEqual obj._EnabledForFiletype('asciidoc'), 0

Execute (Blacklist: disabled for certain filetypes):
  let obj.whitelist = []
  let obj.blacklist = ['text']
  AssertEqual obj._EnabledForFiletype('text'), 0
  AssertEqual obj._EnabledForFiletype('asciidoc'), 1

Execute (Both: blacklist is stronger than whitelist):
  let obj.whitelist = ['text']
  let obj.blacklist = ['text']
  AssertEqual obj._EnabledForFiletype('text'), 0
